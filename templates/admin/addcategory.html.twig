{% extends 'base.html.twig' %}

{% block title %} Ajouter une Catégorie {% endblock %}

{% block body %}

    <h1> Ajouter une Catégorie </h1>

    {{ form_start(form) }}
     
       <div class="row mt-4">
           <div class="col">
             {{ form_label(form.nom, 'Titre') }}
             {{ form_widget(form.nom) }}
           </div>
       </div>
       <div class="row">
          <div class="col">
            {{ form_row(form.description) }}
          </div>
       </div>
{{ form_end(form) }}

{% endblock %}
