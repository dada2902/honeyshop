{% extends 'base.html.twig' %}

{% block title %} Ajouter un utilistaeurs{% endblock %}

{% block body %}

    <h1> Ajouter un utilistaeurs </h1>

    {{ form_start(form) }}
     
       <div class="row mt-4">
           <div class="col">
             {{ form_label(form.nom, 'Nom') }}
             {{ form_widget(form.nom) }}
           </div>
       </div>
       <div class="row">
          <div class="col">
            {{ form_row(form.prenom) }}
          </div>
          <div class="col">
            {{ form_row(form.roles) }}
          </div>
          <div class="col">
            {{ form_row(form.email) }}
          </div>
       </div>
{{ form_end(form) }}

{% endblock %}
